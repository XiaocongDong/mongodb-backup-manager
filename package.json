{
  "name": "mongodb-backup-manager",
  "version": "0.1.0",
  "description": "a webserver to manager the backup and restore of multiple mongoDBs",
  "main": "app.js",
  "target": "node",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "build": "rm -rf ./dist && NODE_ENV=production webpack --progress",
    "dev-client-build": "webpack --config client.webpack.production.config.js --watch --progress",
    "dev-server-build": "webpack --config server.webpack.config.js --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XiaocongDong/mongodb-backup-manager.git"
  },
  "keywords": [
    "mongodb",
    "backup",
    "retore",
    "multiple",
    "real-time",
    "system",
    "notifications"
  ],
  "author": "Xiaocong Dong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/XiaocongDong/mongodb-backup-manager/issues"
  },
  "preferGlobal": true,
  "bin": {
    "mongodb-backup-manager": "./index.js"
  },
  "homepage": "https://github.com/XiaocongDong/mongodb-backup-manager#readme",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-es2015": "^6.24.0",
    "webpack": "^2.2.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "sass-loader": "^6.0.3",
    "node-sass": "^4.0.0",
    "postcss-loader": "^1.3.3",
    "axios": "^0.16.1",
    "immutable": "^3.8.1",
    "moment": "^2.16.0",
    "react": "^15.4.2",
    "react-datetime": "^2.8.8",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "react-select": "^1.0.0-rc.3",
    "react-transition-group": "^1.1.3",
    "recharts": "^0.22.4",
    "redux": "^3.6.0",
